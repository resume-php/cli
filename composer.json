{
    "name": "resume/cli",
    "description": "Generate a beautiful HTML résumé based on a JSON file.",
    "keywords": ["resume", "cv", "cli", "command", "jsonresume", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "^4.0",
        "resume/installer-plugin": "dev-master",
        "resume/default-theme": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "Resume\\Cli\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Resume\\Cli\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": ["resume"]
}
